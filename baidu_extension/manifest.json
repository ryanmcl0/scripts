{
  "manifest_version": 3,
  "name": "Map Switcher",
  "version": "1.0",
  "description": "Switch between Baidu Maps and Google Maps coordinates",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://*.google.com/maps/*", "https://*.baidu.com/*"],
      "js": ["content.js"]
    }
  ],
  "host_permissions": [
    "https://*.google.com/maps/*",
    "https://*.baidu.com/*"
  ]
}